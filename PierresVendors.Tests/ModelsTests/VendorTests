using Microsoft.VisualStudio.TestTools.UnitTesting;

namespace PierresVendors.Tests
{
  [TestClass]
  public class VendorTests
  {
    [TestMethod]
    public void Vendor_CreatesInstanceOfVendor_Vendor()
    {
       Vendor v = new Vendor();
       Assert.AreEqual(typeof(Vendor), v.GetType());
    }
  }

}

